{
  "rules": {
    "no-console": 0,
    "func-names": 0,
    "vars-on-top": 0,
    "no-else-return": 0,
    "consistent-return": 0,
    "no-process-exit": 0
  },
  "globals": {
    "useResultsTable_template": false,
    "useResultsTableTemplate" : true,
    "imageBoxesTemplate": true,
    "casper": false
  },
  "env": {
    "node": true
  },
  "extends": "eslint-config-airbnb-es5",
}
